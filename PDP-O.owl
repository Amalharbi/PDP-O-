#################################################################
#    Object Properties
#################################################################

###  http://www.owl-ontologies.com/DPDAPO#causes
:causes rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        owl:inverseOf :is_Caused_By ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :Pest_Damage
                                   :Plant_Disease
                                 )
                   ] ;
        :definetion "Agent X (organism) brings about a disease or damage Y"@en .


###  http://www.owl-ontologies.com/DPDAPO#finally_Apppears_On
:finally_Apppears_On rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :is_Appear_On .


###  http://www.owl-ontologies.com/DPDAPO#first_Appears_On
:first_Appears_On rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :is_Appear_On .


###  http://www.owl-ontologies.com/DPDAPO#first_Symptom
:first_Symptom rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :has_Symptom .


###  http://www.owl-ontologies.com/DPDAPO#has_Abnormal_Appearance
:has_Abnormal_Appearance rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         owl:inverseOf :is_Appear_On ;
                         rdf:type owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty .


###  http://www.owl-ontologies.com/DPDAPO#has_Active_Time
:has_Active_Time rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :has_Time ;
                 owl:inverseOf :is_Active_Time_Of ;
                 rdf:type owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain <http://www.owl-ontologies.com/PPOntology.owl#Organism> .


###  http://www.owl-ontologies.com/DPDAPO#has_Agricultural_Practice
:has_Agricultural_Practice rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           owl:inverseOf :is_Agricultural_Practice_Of ;
                           rdf:type owl:AsymmetricProperty ,
                                    owl:IrreflexiveProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( :Pest_Damage
                                                       :Plant_Disease
                                                     )
                                       ] ;
                           rdfs:range :Agricultural_Practice ;
                           :definetion "A disease or damage Y occured because of an agricultural practice X"@en .


###  http://www.owl-ontologies.com/DPDAPO#has_Bactericide_Chemical_Control
:has_Bactericide_Chemical_Control rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf :has_Chemical_Control ;
                                  owl:inverseOf :is_Bactericide_Chemical_Control_Of ;
                                  rdf:type owl:AsymmetricProperty ,
                                           owl:IrreflexiveProperty ;
                                  rdfs:domain [ rdf:type owl:Class ;
                                                owl:unionOf ( :Pest_Damage
                                                              :Plant_Disease
                                                            )
                                              ] ;
                                  rdfs:range :Bactericide ;
                                  :definetion "A material (bactericide) X that might prevent or reduce the effect of a disease or damage Y"@en .


###  http://www.owl-ontologies.com/DPDAPO#has_Biological_Control
:has_Biological_Control rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :has_Recommended_Control ;
                        owl:inverseOf :is_Biological_Control_Of ;
                        rdf:type owl:AsymmetricProperty ,
                                 owl:IrreflexiveProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :Pest_Damage
                                                    :Plant_Disease
                                                  )
                                    ] ;
                        rdfs:range :Biological_Control ;
                        :definetion "A biological agents X that might prevent or reduce the effect of a disease or damage Y"@en .


###  http://www.owl-ontologies.com/DPDAPO#has_Characteristic
:has_Characteristic rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    owl:inverseOf :is_Characteristic_Of ;
                    rdf:type owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain :Symptom .


###  http://www.owl-ontologies.com/DPDAPO#has_Chemical_Control
:has_Chemical_Control rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :has_Recommended_Control ;
                      owl:inverseOf :is_Chemical_Control_Of ;
                      rdf:type owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:range :Chemical_Control ;
                      :definetion "A material (chemical) X that might prevent or reduce the effect of a disease or damage Y"@en .


###  http://www.owl-ontologies.com/DPDAPO#has_Continuous_Period_Of_Time
:has_Continuous_Period_Of_Time rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :has_Time ;
                               rdf:type owl:AsymmetricProperty ,
                                        owl:IrreflexiveProperty ;
                               rdfs:domain :Meteorological_Factor .


###  http://www.owl-ontologies.com/DPDAPO#has_Cultural_Control
:has_Cultural_Control rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :has_Recommended_Control ;
                      owl:inverseOf :is_Cultural_Control_Of ;
                      rdf:type owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:range :Cultural_Control ;
                      :definetion "An action (Physical) X that might prevent or reduce the effect of a disease or damage Y"@en .


###  http://www.owl-ontologies.com/DPDAPO#has_Disease
:has_Disease rdf:type owl:ObjectProperty ;
             owl:inverseOf :is_Disease_Of ;
             rdf:type owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty .


