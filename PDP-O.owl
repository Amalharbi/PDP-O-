                       owl:inverseOf :is_Intervention_Time_Of ;
                       rdf:type owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty .


###  http://www.owl-ontologies.com/DPDAPO#has_Meteorological_Factor
:has_Meteorological_Factor rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :has_Preferable_Environmental_Factor ;
                           owl:inverseOf :is_Meteorological_Factor_Of ;
                           rdf:type owl:AsymmetricProperty ,
                                    owl:IrreflexiveProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( :Pest_Damage
                                                       :Plant_Disease
                                                     )
                                       ] ;
                           rdfs:range :Meteorological_Factor ;
                           :definetion "A disease or damage Y occured because of meteorological factor X"@en .


###  http://www.owl-ontologies.com/DPDAPO#has_Outbreak_Time
:has_Outbreak_Time rdf:type owl:ObjectProperty ;
